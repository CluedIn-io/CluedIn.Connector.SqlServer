<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Description>CluedIn.Connector.SqlServer provides connectivity to a Microsoft Sql Server database</Description>
    <AssemblyName>CluedIn.Connector.SqlServer</AssemblyName>
    <RootNamespace>CluedIn.Connector.SqlServer</RootNamespace>
    <IsCluedInServerComponent>true</IsCluedInServerComponent>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="Resources\microsoft-sql-server-logo.svg" />
  </ItemGroup>
  <ItemGroup>
    <EmbeddedResource Include="Resources\microsoft-sql-server-logo.svg" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="CluedIn.Connector.Common" Version="3.2.5" />
    <PackageReference Include="CluedIn.Core" />
    <PackageReference Include="ComponentHost" />
  </ItemGroup>

